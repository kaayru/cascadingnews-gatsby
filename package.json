  },
  "scripts": {
    "build": "npx --node-arg '-r esm' gatsby build",
    "clean": "gatsby clean",
    "develop": "npx --node-arg '-r esm' gatsby develop",
    "eslint": "npx eslint . --ext tsx,ts,js",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "codegen": "graphql-codegen --config codegen.yml",
    "serve": "npx --node-arg '-r esm' gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "type-check": "tsc --noEmit --skipLibCheck",
    "prepush": "yarn eslint && yarn type-check && yarn build && yarn test"
